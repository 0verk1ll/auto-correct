{
  "name": "auto-correct",
  "displayName": "Auto Correct",
  "description": "Auto corrects your common type errors",
  "version": "0.2.0",
  "publisher": "sygene",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:type"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/genesy/auto-correct"
  },
  "icon": "icon.png",
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Auto Correct Configuration",
      "properties": {
        "auto-correct.dictionary": {
          "type": "array",
          "default": [
            {
              "languages": [
                "*"
              ],
              "words": {
                "hte": "the",
                "{despa,sepe}rat{e,es,ed,ing,ely,ion,ions,or}": "{despe,sepa}rat{}"
              },
              "useLargeList": true
            },
            {
              "languages": [
                "javascript",
                "javascriptreact",
                "typescript",
                "typescriptreact"
              ],
              "words": {
                "cosnt": "const",
                "functoin": "function"
              }
            }
          ],
          "scope": "resource",
          "description": "List of misspelled words to correct"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^8.10.25",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.6.5",
    "vscode": "^1.1.36",
    "vscode-test": "^1.3.0"
  }
}
