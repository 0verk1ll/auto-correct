{
  "name": "auto-correct",
  "displayName": "Auto Correct",
  "description": "Auto corrects your common type errors",
  "version": "0.2.0",
  "publisher": "sygene",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:type"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/genesy/auto-correct"
  },
  "icon": "icon.png",
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Auto Correct Configuration",
      "properties": {
        "auto-correct.dictionary": {
          "type": "array",
          "default": [
            {
              "languages": [
                "*"
              ],
              "words": {},
              "useLargeList": false
            },
            {
              "languages": [
                "javascript",
                "javascriptreact",
                "typescript",
                "typescriptreact"
              ],
              "words": {
                "cosnt": "const",
                "functoin": "function"
              }
            }
          ],
          "scope": "resource",
          "description": "List of misspelled words to correct"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.6.5",
    "vscode": "^1.1.21",
    "tslint": "^6.0.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
